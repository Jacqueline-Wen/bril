@main {
.entry:
  i.0: int = const 1;
  cond.entry: bool = undef;
  set cond.0 cond.entry;
  max.entry: int = undef;
  set max.0 max.entry;
  set i.1 i.0;
  jmp .loop;
.loop:
  i.1: int = get;
  max.0: int = get;
  cond.0: bool = get;
  max.1: int = const 10;
  cond.1: bool = lt i.1 max.1;
  br cond.1 .body .exit;
.body:
  i.2: int = add i.1 i.1;
  set cond.0 cond.1;
  set max.0 max.1;
  set i.1 i.2;
  jmp .loop;
.exit:
  print i.1;
  ret;
}
