@main {
.b1:
  cond.0: bool = const true;
  br cond.0 .true .false;
.true:
  a.0: int = const 0;
  b.true: int = undef;
  set b.1 b.true;
  set a.1 a.0;
  jmp .zexit;
.false:
  b.0: int = const 1;
  set b.1 b.0;
  a.false: int = undef;
  set a.1 a.false;
  jmp .zexit;
.zexit:
  a.1: int = get;
  b.1: int = get;
  print a.1;
  ret;
}
